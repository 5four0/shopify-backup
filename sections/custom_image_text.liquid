<style>
  .custom-section{
    padding: 70px 0;
    background:#F9F9F9;
  }
  .image_text_container{
    max-width:100%;
    /* padding:0 100px; */
  }
  .custom_text, .custom_image{
    flex:0 0 50%;
    position:relative;
  }
  .custom-section .section__heading{
    font-size:20px;
  }
  .image_text_c{
    border-radius:30px;
    overflow:hidden;
  }
  .custom_image img{
    width:100%;
    height:495px;
    object-fit:cover;
        border-top-right-radius: 30px;
    border-bottom-right-radius: 30px;
  }
  .custom_text{
    display:flex;
    flex-direction:column;
    justify-content:center;
    padding:0 48px;
    background:white;
  }
  .image_text .section__header{
  margin-bottom: 12px;
}
  .custom-section .reading-width--inline {
    margin-bottom: 32px;
  }

.custom_text_item{
  margin-bottom:32px;
}
.custom_text_item{
  opacity:1;
  transition:.5s;
  cursor:pointer;
}  
  .custom_text_item svg{
    transition:.5s;
    opacity:0;
    transform:translateX(-40px);
  }
  
 .custom_text_item:hover{
   opacity:1;
 }
  .custom_text_item:hover svg{
     opacity:1;
    transform:translateX(0px);
  }
 .custom_text_item p{
   font-size:14px;
   color:#8EB3C6;
     margin-bottom:12px;
   
 } 
  .custom_text_item a{
        display: flex;
    justify-content: space-between;
    font-weight:bold;
  }
  .custom_text .btn{
    width:fit-content;
    padding:10px 32px;
    font-size: 12px;
    font-style: normal;
    font-weight: 600;
    line-height: 18px; /* 18px */
    letter-spacing: 1px;
    text-transform: uppercase;
    border-radius: var(--Button-mombile-rounded, 12px);
  }
  .custom_text a.btn.btn--secondary:hover {
      background: #000;
      border-color: #000;
  }

  .custom_text .custom_text_item:not(:last-child) {
        margin-bottom: 16px;
    }
  .custom_text .custom_text_item:last-child {
       margin-bottom: 32px;
    }
  .custom_text .custom_text_item:nth-child(3) {
       margin-bottom: 32px;
    }
  .custom_image img{
    width:100%;
    transition:.5s;
  }
  .custom_image .seconds{
   position:absolute;
    height:100%;
    left:0;
    width:100%;
    top:0;
    opacity:0;
    
  }
  .custom_image .seconds img{
    width:100%;
    height:100%;
    object-fit:cover;
  }
  @media (max-width:999px){
     .custom-section{
    padding:38px 0;
  }
    .custom_text_item{
      opacity:1;
    }
   
    .custom_text_item {
    margin-bottom: 8px;
}
    .custom-section .section__heading {
    font-size: 18px;
}
    .image_text .section__header {
    margin-bottom: 8px;
}
    .image_text .reading-width--inline {
    margin-bottom: 24px;
    font-size: 14px;
}
    .container{
      padding:0 24px;
    }
    .custom_text .btn {

    margin-top: 16px;
}
  }
  @media (max-width:769px){
     .custom_text, .custom_image{
    flex:0 0 100%;
  }
    .custom_text_item svg{
    transform:translateX(-20px);
  }
    .image_text_c{
      flex-direction:column-reverse;
    }
    .image_text_c {
    border-radius: 15px;
}
    .custom_image img{
        border-top-right-radius: 15px;
      border-top-left-radius: 15px;
    border-bottom-right-radius: 0px;
       height:244px;
  }
    .custom_text {
    padding: 40px 20px;
}
    .custom_text_item p {
    font-size: 12px;
}
    .custom_text_item a {
    font-size: 14px;
    font-weight: bold;
}
  }
    @media (max-width:430px) {
      .custom_text_item svg {
          opacity: 1;
          transform: unset;
      }
    }

  .custom-section .custom_{{ section.id }} .section__heading {
    font-size: {{section.settings.h_font-size_d}}px;
    font-style: normal;
    font-weight: 600;
    line-height: 41.6px; /* 41.6px */
    letter-spacing: 2px;
    text-transform: capitalize;
}
   .custom_{{ section.id }} .reading-width--inline p {
    font-size: {{section.settings.s_font-size_d}}px;
} 
  .custom_{{ section.id }} .custom_text_item p {
    font-size: {{section.settings.small_t_font-size_d}}px;
    font-style: normal;
    font-weight: 400;
    line-height: 21px; /* 21px */

}
  .custom_{{ section.id }} .custom_text_item a {
    font-size: {{section.settings.big_t_font-size_d}}px;
    font-style: normal;
    font-weight: 600;
    line-height: 24px; /* 24px */
}
  @media (max-width:999px){
    .custom-section  .custom_{{ section.id }} .section__heading {
    font-size: {{section.settings.h_font-size_m}}px;
      font-style: normal;
    font-weight: 600;
    line-height: 41.6px; /* 41.6px */
    letter-spacing: 2px;
    text-transform: capitalize;
}
   .custom_{{ section.id }} .reading-width--inline p {
    font-size: {{section.settings.s_font-size_m}}px;
    font-style: normal;
    font-weight: 400;
    line-height: 21px; 
} 
  .custom_{{ section.id }} .custom_text_item p {
    font-size: {{section.settings.small_t_font-size_m}}px;
}
  .custom_{{ section.id }} .custom_text_item a {
    font-size: {{section.settings.big_t_font-size_m}}px;
}
  }

   @media only screen and (max-width: 430px) {
         .custom_text {
        padding: 48px 24px;
    }
    .custom_{{ section.id }} .reading-width--inline p {
    font-size: {{section.settings.s_font-size_m}}px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; 
    } 
    .custom-section .custom_{{ section.id }} .section__heading {
         font-size: {{section.settings.h_font-size_m}}px;
        font-style: normal;
        font-weight: 600;
        line-height: 33.6px; 
        letter-spacing: 2px;
        text-transform: capitalize;
    }
     .custom_{{ section.id }} .custom_text_item p {
      font-size: {{section.settings.small_t_font-size_m}}px;
       line-height: 18px;
    }
      .custom_{{ section.id }} .custom_text_item a {
        font-size: {{section.settings.big_t_font-size_m}}px;
         line-height: 21px;
    }
      .custom_text .custom_text_item:nth-child(3) {
       margin-bottom: 24px;
    }
     .custom-section{
        padding: 30px 0;
      }
  }

  @media only screen and (max-width: 414px) {
      .custom-section{
        padding: 30px 0;
      }
        .custom_text {
        padding: 48px 24px;
    }
    .custom_{{ section.id }} .reading-width--inline p {
    font-size: {{section.settings.s_font-size_m}}px;
    font-style: normal;
    font-weight: 400;
    line-height: 18px; 
    } 
    .custom-section .custom_{{ section.id }} .section__heading {
         font-size: {{section.settings.h_font-size_m}}px;
        font-style: normal;
        font-weight: 600;
        line-height: 33.6px; 
        letter-spacing: 2px;
        text-transform: capitalize;
    }
     .custom_{{ section.id }} .custom_text_item p {
      font-size: {{section.settings.small_t_font-size_m}}px;
       line-height: 18px;
    }
      .custom_{{ section.id }} .custom_text_item a {
        font-size: {{section.settings.big_t_font-size_m}}px;
         line-height: 21px;
    }
      .custom_text .custom_text_item:nth-child(3) {
       margin-bottom: 24px;
    }
  }
</style>
<div class="custom_{{ section.id }}">
<div class="container image_text">
    <div class="section__header items-center flex flex-col text-center gap-x-theme cc-animate-init cc-animate-in" data-cc-animate="">
      <h2 class="h4 section__heading mb-0 flex-auto text-center">{{ section.settings.heading }}</h2>
    </div>
    <div class="reading-width--inline md-down:px-gutter rte mx-auto text-center">
          <p>{{ section.settings.subheading }}</p>
    </div>
</div>
<div class="container image_text_container">
<div class="flex flex-wrap image_text_c">
     <div class="cc-animate-init cc-animate-in custom_text">
       {% for block in section.blocks %}
       <div class="custom_text_item" data_index="{{ forloop.index }}">
         

         <p>{{ block.settings.small_text }}</p>
         <a href="{% if block.settings.link != blank %}{{ block.settings.link }}{% else %}#{% endif %}">
           {{ block.settings.big_text }}{% if block.settings.link != blank %}<span>{% render 'icon-arrow-right' %}</span>{% endif %}
         </a>
       </div>
         {% endfor %}

      {% if section.settings.link != blank %}
       <a href="{{ section.settings.link }}" class="btn btn--{{ section.settings.cta_style }}">{{ section.settings.link_text }}</a>
      {% endif %}
       
     </div>
    <div class="cc-animate-init cc-animate-in custom_image">
      {% for block in section.blocks %}
      <img data_index="{{ forloop.index }}" {% if forloop.first %}{% else %}class="seconds"{% endif %} src="{{ block.settings.image | img_url:'master' }}">
      {% endfor %}
    </div>
</div>
</div>
</div>
<script>
  document.querySelectorAll(".custom_text_item").forEach(function(item){
    item.addEventListener("mouseenter",function(){
          document.querySelectorAll(".custom_text_item").forEach(function(items){
                items.style.opacity=0.5;
          })
          document.querySelectorAll(".custom_text_item").forEach(function(items){
                items.style.opacity=0.5;
          })
          document.querySelectorAll(".custom_image img").forEach(function(items){
                items.style.opacity=0;
          })
          item.style.opacity=1;
        document.querySelector(".custom_image img[data_index='" + item.getAttribute("data_index") +  "']").style.opacity = 1;    
      
    })
        item.addEventListener("mouseleave",function(){
          document.querySelectorAll(".custom_text_item").forEach(function(items){
                items.style.opacity=1;
          })
      
    })
  })
</script>
{% schema %}
{
  "name": "custom image text",
  "tag": "section",
  "class":"custom-section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "heading"
    },
      {
      "type": "range",
      "id": "h_font-size_d",
      "label": "font-size desktop",
      "min":10,
      "max":100,
      "default":20
    },
          {
      "type": "range",
      "id": "h_font-size_m",
      "label": "font-size mobile",
      "min":10,
      "max":100,
      "default":18
    },
   {
      "type": "text",
      "id": "subheading",
      "label": "subheading"
   },
    {
      "type": "range",
      "id": "s_font-size_d",
      "label": "font-size desktop",
      "min":10,
      "max":100,
      "default":16
    },
          {
      "type": "range",
      "id": "s_font-size_m",
      "label": "font-size mobile",
      "min":10,
      "max":100,
      "default":14
    },
     {
       "type": "image_picker",
      "id": "image",
      "label": "image"
     },
    {
      "type": "url",
      "id": "link",
      "label": "link"
    },
    {
      "type": "text",
      "id": "link_text",
      "label": "link text"
    },
      {
      "type": "select",
      "id": "cta_style",
      "label": "button style",
      "options": [
        {
          "value": "primary",
          "label": "primary"
        },
        {
          "value": "secondary",
          "label": "secondary"
        }
      ],
        "default":"secondary"
        },
    {
      "type": "range",
      "id": "small_t_font-size_d",
      "label": "content small font size desktop",
      "min":10,
      "max":100,
      "default":14
    },
          {
      "type": "range",
      "id": "small_t_font-size_m",
      "label": "content small font-size mobile",
      "min":10,
      "max":100,
      "default":12
    },
        {
      "type": "range",
      "id": "big_t_font-size_d",
      "label": "content big font size desktop",
      "min":10,
      "max":100,
      "default":16
    },
          {
      "type": "range",
      "id": "big_t_font-size_m",
      "label": "content big font-size mobile",
      "min":10,
      "max":100,
      "default":14
    }
  ],
  "blocks": [
    {
      "type":"text_block",
      "name":"text block",
      "settings": [
        {
       "type": "image_picker",
      "id": "image",
      "label": "image"
     },
        {
      "type": "text",
      "id": "small_text",
      "label": "small text"
    },
    {
      "type": "url",
      "id": "link",
      "label": "link"
    },
    {
      "type": "text",
      "id": "big_text",
      "label": "big text"
    }
      ]
    }
  ],
  "presets": [
    {
      "name": "custom image text"
    }
  ]
}
{% endschema %}