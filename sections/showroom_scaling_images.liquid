<style>
  html{
            --size: 1600;
        font-size: calc(9*100vw/var(--size));
  }
body{
      font-size: 1.9rem;
}
 .scale-grid-wrap {
    position: sticky;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
   --vh: 9.53px;
    height: calc(var(--vh, 1vh)*100);
    font-size: 1rem;
}
  .overflow-hidden {
    overflow: hidden;
}
  .scale-grid-content {
    position: absolute;
    top: 50%;
    left: 50%;
    --tw-translate-y: -50%;
    --tw-translate-x: -50%;
    --tw-rotate:0;
    --tw-skew-x:0;
    --tw-skew-y:0;
    --tw-scale-x:1;
    --tw-scale-y:1;
    transform: translate(var(--tw-translate-x), var(--tw-translate-y)) rotate(var(--tw-rotate)) skewX(var(--tw-skew-x)) skewY(var(--tw-skew-y)) scaleX(var(--tw-scale-x)) scaleY(var(--tw-scale-y));
    width: 129.6em;
    height: 127.8em;
}
.scale-grid-item {
    position: absolute;
    background-color: #111;
    overflow: hidden;
  border-radius:30px;
}
 .scale-grid-item:nth-child(1) {
    top: 0;
    left: 32.9em;
    width: 77em;
    height: 41.2em;
}
  .scale-grid-item img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    -o-object-fit: cover;
    object-fit: cover;
}
.scale-grid-item:nth-child(2) {
    top: 50.3em;
    left: 0;
    width: 30.9em;
    height: 34.1em;
}
.scale-grid-item:nth-child(4) {
    top: 50.3em;
    right: 0;
    width: 30.9em;
    height: 34.1em;
}
  .scale-grid-item:nth-child(5) {
    bottom: 12.6em;
    left: 6.6em;
    width: 37.5em;
    height: 28.8em;
}
  .scale-grid-item:nth-child(6) {
    bottom: 0;
    left: 46.1em;
    width: 63.8em;
    height: 41.2em;
}
  .scale-grid-item:nth-child(3) {
    width: 100%;
}
  .scale-grid-item:nth-child(3) {
    height: 100vh;
    height: calc(var(--vh, 1vh)*100);
}
 @media (min-width: 650px) {
    .scale-grid {
        height: 400vh;
    }
  .s\:px-0 {
    padding-left: 0rem;
    padding-right: 0rem;
}
  .s\:block {
    display: block;
}
   .scale-grid-item:nth-child(3) {
    top: 43.2em;
    left: 32.9em;
}
   .scale-grid-item:nth-child(3) {
    width: 63.8em;
}
   .scale-grid-item:nth-child(3) {
    height: 41.2em;
    margin-left: 0rem;
}
} 
@media (max-width:649px){
  .scale-grid-content{
        position: relative;
    width: 100%;
    padding: 0 24px;
  }
  .scale-grid-item{
      display:none;
    }
.scale-grid-item:nth-child(3) {
      display: block;
    position: relative;
    width: 100%;
   height: 50vh;
}
}
</style>
